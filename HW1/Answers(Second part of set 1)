2:
__________________
#include <iostream>
using namespace std;
int main() {
  int n;
  cin >> n;
  if (n == 1){
      cout << "No\n";
      return 0;

  }
  for (int i = 2; i < n; i++){
      if (n%i == 0){
          cout << "No\n";
          return 0;
    
      }
  }
  cout << "Yes\n";
  return 0;
}
